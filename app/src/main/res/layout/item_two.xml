<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/item_two_textview1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_20"
        android:fontFamily="sans-serif-light"
        android:textColor="#303148"
        android:textSize="@dimen/text_size_19"
        android:textStyle="normal"
        app:layout_constraintEnd_toStartOf="@+id/item_two_textview2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Reference Number" />

    <TextView
        android:id="@+id/item_two_textview2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_20"
        android:fontFamily="sans-serif"
        android:maxLines="1"
        android:singleLine="true"
        android:textColor="#f39798a4"
        android:textSize="@dimen/text_size_19"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/item_two_textview1"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="test1 " />
</androidx.constraintlayout.widget.ConstraintLayout>